{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:all": "npm run style:lint && npm run style:prettier",
    "style:lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "style:prettier": "prettier --check . --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx",
    "style:fix": "prettier --write . --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "husky": "^8.0.1",
    "next": "^12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-moralis": "^1.4.2",
    "react-router": "^6.4.2",
    "web3uikit": "^1.0.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.11",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.1",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.8"
  }
}
